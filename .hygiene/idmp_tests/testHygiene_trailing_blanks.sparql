##
# banner No literal should contain trailing space(s).

SELECT DISTINCT ?warning ?value
WHERE
{
	?resource ?property ?value.
	FILTER (ISLITERAL(?value))
	FILTER (REGEX(?value, '.+[\\u0020\\t]+$'))
	FILTER regex(str(?resource), "${NAMESPACE_REGEX}")
	BIND (concat ("ERROR: ", "Resource <", STR(?resource), "> has property <", STR(?property), "> whose value contains trailing space(s):") AS ?warning)
}
